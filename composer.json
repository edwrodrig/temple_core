{
  "name": "labo86/temple_core",
  "description": "Una biblioteca para procesar plantillas de proyectos",
  "authors": [
    {
      "name": "Edwin Rodríguez-León",
      "homepage": "http://www.edwin.cl",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "labo86/exception_with_data": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "labo86\\temple_core\\": "src/"
    },
    "exclude-from-classmap": ["src/PharBuilder.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "test\\labo86\\temple_core\\": "test/"
    },
    "files" : [
      "src/PharBuilder.php"
    ]
  },
  "archive": {
    "exclude": [
      "/.git",
      "/.idea",
      "/examples",
      "/scripts",
      "/src/PharBuilder.php",
      "/test",
      "/.gitignore",
      "/.phpunit.result.cache",
      "/phpunit.xml.dist",
      "/README.md",
      "/.travis.yml"
    ]
  }
}
